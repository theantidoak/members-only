{
  "name": "members-only",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node ./dist/bin/www",
    "dev": "concurrently \"nodemon --exec ts-node ./src/bin/www.ts\" \"npm run scss\"",
    "build": "npx tsc && ncp src/views dist/views && ncp public dist/public",
    "scss": "sass --watch src/scss/style.scss public/stylesheets/style.css"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/debug": "^4.1.12",
    "@types/morgan": "^1.9.9",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.19.2",
    "http-errors": "~1.6.3",
    "mongoose": "^8.2.4",
    "morgan": "~1.9.1",
    "sass": "^1.72.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.2",
    "concurrently": "^8.2.2",
    "ncp": "^2.0.0",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
